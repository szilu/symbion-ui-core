{
  "name": "@symbion/ui-core",
  "version": "0.0.3",
  "description": "Symbion UI core components",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "css"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./scroll.js": {
      "import": "./lib/scroll.js",
      "types": "./lib/scroll.d.ts"
    },
    "./scroll.css": "./css/scroll.css",
    "./sortable.js": {
      "import": "./lib/sortable.js",
      "types": "./lib/sortable.d.ts"
    },
    "./datatable.js": {
      "import": "./lib/datatable.js",
      "types": "./lib/datatable.d.ts"
    },
    "./datatable.css": "./css/datatable.css"
  },
  "scripts": {
    "watch": "tsc --watch --outDir lib -p tsconfig.json",
    "build": "tsc --outDir lib -p tsconfig.json",
    "clean": "rimraf .cache lib",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szilu/symbion-ui-core"
  },
  "author": "Szil√°rd Hajba <szilu@symbion.hu>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0",
    "react-dnd": ">=16.0",
    "react-dom": ">=18.0"
  },
  "devDependencies": {
    "@symbion/runtype": "^0.5.6",
    "@symbion/simple-form": "workspace:^",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "csstype": "^3.1.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
