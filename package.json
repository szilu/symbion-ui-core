{
  "name": "@symbion/ui-core",
  "version": "0.0.1",
  "description": "Symbion UI core components",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./scroll": {
      "import": "./lib/scroll.js",
      "types": "./lib/scroll.d.ts"
    },
    "./sortable.js": {
      "import": "./lib/sortable.js",
      "types": "./lib/sortable.d.ts"
    },
    "./sortable": {
      "import": "./lib/sortable.js",
      "types": "./lib/sortable.d.ts"
    },
    "./datatable": {
      "import": "./lib/datatable.js",
      "types": "./lib/datatable.d.ts"
    }
  },
  "scripts": {
    "watch": "tsc --watch --outDir lib -p tsconfig.json",
    "build": "tsc --outDir lib -p tsconfig.json",
    "clean": "rimraf .cache lib",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --access public"
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/szilu/symbion-ui-core"
  },
  "author": "Szil√°rd Hajba <szilu@symbion.hu>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0",
    "react-dnd": ">=16.0",
    "react-dom": ">=18.0"
  },
  "devDependencies": {
    "@symbion/runtype": "^0.5.3",
    "@symbion/simple-form": "^0.9.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "csstype": "^3.1.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "rimraf": "^4.1.3",
    "typescript": "^4.9.5"
  }
}
